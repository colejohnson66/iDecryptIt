<Window x:Class="Hexware.Programs.iDecryptIt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Hexware.Programs.iDecryptIt"
        Title="iDecryptIt"
        Icon="icon.ico"
        Height="600" Width="800"
        MinHeight="600" MinWidth="800"
        Background="{StaticResource Background}"
        Loaded="Window_Loaded"
        Closing="Window_Closing">
    <Window.Resources>
        <GridLength x:Key="keyTitleWidth">125</GridLength>
        <GridLength x:Key="keyFilenameWidth">150</GridLength>
    </Window.Resources>
    <Window.TaskbarItemInfo>
        <TaskbarItemInfo />
    </Window.TaskbarItemInfo>
    <Grid Margin="3">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid>
            <Grid Margin="6" Background="{StaticResource GrayBorder}">
                <Grid Margin="3" Background="White">
                    <ProgressBar Visibility="Hidden" x:Name="progDecrypt" />
                    <Grid Margin="6" x:Name="gridDmg">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="3*" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="3*" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="2*" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="Auto" MinWidth="75" />
                        </Grid.ColumnDefinitions>
                        <Button x:Name="btnDecrypt" Click="btnDecrypt_Click">
                            <Grid Height="32">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="32" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="6" />
                                </Grid.ColumnDefinitions>
                                <Image Source="img/changes-allow@32.png" />
                                <TextBlock Grid.Column="2" Text="Decrypt" VerticalAlignment="Center" x:Name="btnDecryptText" />
                            </Grid>
                        </Button>
                        <Grid Grid.Column="2" Height="24">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="txtInputLabel" Text="Input File:  " VerticalAlignment="Center" />
                            <TextBox x:Name="textInputFileName" Grid.Column="1" TextChanged="TextInputFileName_TextChanged" />
                        </Grid>
                        <Grid Grid.Column="4" Height="24">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="txtOutputLabel" Text="Output File:  " VerticalAlignment="Center" />
                            <TextBox x:Name="textOutputFileName" Grid.Column="1" />
                        </Grid>
                        <Grid Grid.Column="6" Height="24">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="textDecryptLabel" Text="Key:  " VerticalAlignment="Center" />
                            <TextBox x:Name="textDecryptKey" Grid.Column="1" />
                        </Grid>
                        <Button x:Name="btnSelectRootFSInputFile" Content="Select Input File..." Grid.Column="8" Height="24" Click="btnSelectRootFSInputFile_Click" />
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
        <Grid Grid.Row="1">
            <Grid Margin="6" Background="{StaticResource GrayBorder}">
                <Grid Margin="3" Background="White">
                    <Grid Margin="6" x:Name="gridExtract">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="Auto" MinWidth="75" />
                        </Grid.ColumnDefinitions>
                        <Button x:Name="btnExtract" Click="btnExtract_Click">
                            <Grid Height="32">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="32" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="6" />
                                </Grid.ColumnDefinitions>
                                <!--<Image Source="img/7zip@32.png" />-->
                                <TextBlock Grid.Column="2" Text="Extract" VerticalAlignment="Center" x:Name="btnExtractText" />
                            </Grid>
                        </Button>
                        <Grid Grid.Column="2" Height="24">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="txt7ZInputLabel" Text="Input File:  " VerticalAlignment="Center" />
                            <TextBox x:Name="text7ZInputFileName" Grid.Column="1" />
                        </Grid>
                        <Grid Grid.Column="4" Height="24">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="txt7ZOutputLabel" Text="Output Folder:  " VerticalAlignment="Center" />
                            <TextBox x:Name="text7ZOuputFolder" Grid.Column="1" />
                        </Grid>
                        <Button x:Name="btnSelect7ZInputFile" Content="Select Input File..." Grid.Column="6" Height="24" Click="btnSelect7ZInputFile_Click" />
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
        <Grid Grid.Row="2">
            <Grid Margin="6" Background="{StaticResource GrayBorder}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid Margin="3,3,1,3" Background="White">
                    <Grid Margin="6" x:Name="gridIdentify">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="Auto" MinWidth="75" />
                        </Grid.ColumnDefinitions>
                        <Button x:Name="btnWhatAmI" Click="btnWhatAmI_Click">
                            <Grid Height="32">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="32" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Image Source="img/Help-browser@32.png" />
                                <TextBlock Grid.Column="1" Text="Identify Version" VerticalAlignment="Center" Margin="6,0" />
                            </Grid>
                        </Button>
                        <Grid Grid.Column="2" Height="24">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="textWhatAmIFileLabel" Text="File Name:  " VerticalAlignment="Center" />
                            <TextBox x:Name="textWhatAmIFileName" Grid.Column="1" />
                        </Grid>
                        <Button x:Name="btnSelectWhatAmIFile" Content="Select File..." Grid.Column="4" Height="24" Click="btnSelectWhatAmIFile_Click" />
                    </Grid>
                </Grid>
                <Grid Margin="2,3,3,3" Background="White" Grid.Column="1">
                    <Grid Margin="6" x:Name="gridExtras">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Button x:Name="btnREADME" Grid.Column="0" ToolTip="Readme" Click="btnReadme_Click">
                            <Grid Height="32">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="32" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Image Source="img/Help-browser@32.png" />
                                <TextBlock Grid.Column="1" Margin="6,0" Text="Readme" VerticalAlignment="Center" x:Name="btnReadmeText" />
                            </Grid>
                        </Button>
                        <Button Grid.Column="2" x:Name="btnChangelog" Click="btnChangelog_Click">
                            <Grid Height="32">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="32" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Image Source="img/logviewer@32.png" />
                                <TextBlock Grid.Column="1" Margin="6,0" Text="Changelog" VerticalAlignment="Center" x:Name="btnChangelogText" />
                            </Grid>
                        </Button>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
        <Grid Grid.Row="3">
            <Grid Margin="6" Background="{StaticResource GrayBorder}">
                <Grid Margin="3" Background="White">
                    <Grid Margin="6" x:Name="gridVersion">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0" Height="24">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="txtSelectDevice" Text="Device:  " VerticalAlignment="Center" />
                            <ComboBox x:Name="cmbDeviceDropDown" Grid.Column="1"
                                      DisplayMemberPath="Value" SelectedValuePath="ID"
                                      SelectedValue="{Binding DevicesViewModel.Value}"
                                      SelectionChanged="cmbDeviceDropDown_SelectionChanged" />
                        </Grid>
                        <Grid Grid.Column="2" Height="24">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="txtSelectModel" Text="Model:  " VerticalAlignment="Center" />
                            <ComboBox x:Name="cmbModelDropDown" Grid.Column="1" IsEnabled="False"
                                      DisplayMemberPath="Value" SelectedValuePath="ID"
                                      SelectedValue="{Binding ModelsViewModel.Value}"
                                      SelectionChanged="cmbModelDropDown_SelectionChanged" />
                        </Grid>
                        <Grid Grid.Column="4" Height="24">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="txtSelectVersion" Text="Version:  " VerticalAlignment="Center" />
                            <ComboBox x:Name="cmbVersionDropDown" Grid.Column="1" IsEnabled="False"
                                      SelectedValuePath="ID"
                                      SelectedValue="{Binding VersionsViewModel.Value}"
                                      SelectionChanged="cmbVersionDropDown_SelectionChanged">
                                <!-- Set the text to red if there aren't any keys -->
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock x:Name="cmbVersionDropDownTxt" Text="{Binding Path=Value}">
                                            <TextBlock.Style>
                                                <Style TargetType="TextBlock">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding Path=HasKeys}" Value="false">
                                                            <DataTrigger.Setters>
                                                                <Setter Property="Foreground" Value="Red" />
                                                            </DataTrigger.Setters>
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </TextBlock.Style>
                                        </TextBlock>
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                        </Grid>
                        <Button Grid.Column="6" x:Name="btnViewKeys" Click="btnViewKeys_Click">
                            <Grid Height="32">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="32" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Image Source="img/dialog-password@32.png" />
                                <TextBlock Grid.Column="1" Margin="6,0" Text="View Keys" VerticalAlignment="Center" x:Name="btnViewKeysText" />
                            </Grid>
                        </Button>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
        <Grid Grid.Row="4">
            <Grid Margin="6" Background="{StaticResource GrayBorder}">
                <Grid Margin="3" Background="White">
                    <ScrollViewer Margin="6">
                        <Grid Margin="0,0,6,0" x:Name="gridKeys">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0" Text="Device" x:Name="txtDevice" FontSize="32" />
                            <TextBlock Grid.Row="1" Text="Version (Build)" x:Name="txtVersion" FontSize="16" Margin="12,6,0,6" />
                            <Grid Grid.Row="2" x:Name="grdRootFS" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Root FS Key" x:Name="lblRootFSKey" VerticalAlignment="Center" />
                                <TextBox Grid.Column="2" x:Name="keyRootFS" IsReadOnly="True" />
                                <TextBox Grid.Column="4" x:Name="fileRootFS" IsReadOnly="True" />
                            </Grid>
                            <Grid Grid.Row="3" x:Name="grdRestore" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="6" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <TextBlock Text="Restore Ramdisk (IV)" x:Name="lblRestoreIV" VerticalAlignment="Center" />
                                <TextBlock Text="Restore Ramdisk (Key)" x:Name="lblRestoreKey" VerticalAlignment="Center" Grid.Row="2" />
                                <TextBox Grid.Column="2" x:Name="keyRestoreIV" IsReadOnly="True" />
                                <TextBox Grid.Column="2" x:Name="keyRestoreKey" IsReadOnly="True" Grid.Row="2" />
                                <TextBox Grid.Column="4" x:Name="fileRestore" IsReadOnly="True" Grid.RowSpan="3" VerticalAlignment="Center" />
                            </Grid>
                            <Grid Grid.Row="4" x:Name="grdRestoreNoEncrypt" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Restore Ramdisk" x:Name="lblRestoreNoEncrypt" VerticalAlignment="Center" />
                                <TextBlock Grid.Column="2" Text="Not Encrypted" x:Name="keyRestoreNoEncrypt" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                <TextBox Grid.Column="4" x:Name="fileRestoreNoEncrypt" IsReadOnly="True" />
                            </Grid>
                            <Grid Grid.Row="5" x:Name="grdUpdate" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="6" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <TextBlock Text="Update Ramdisk (IV)" x:Name="lblUpdateIV" VerticalAlignment="Center" />
                                <TextBlock Text="Update Ramdisk (Key)" x:Name="lblUpdateKey" VerticalAlignment="Center" Grid.Row="2" />
                                <TextBox Grid.Column="2" x:Name="keyUpdateIV" IsReadOnly="True" />
                                <TextBox Grid.Column="2" x:Name="keyUpdateKey" IsReadOnly="True" Grid.Row="2" />
                                <TextBox Grid.Column="4" x:Name="fileUpdate" IsReadOnly="True" Grid.RowSpan="3" VerticalAlignment="Center" />
                            </Grid>
                            <Grid Grid.Row="6" x:Name="grdUpdateNoEncrypt" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Update Ramdisk" x:Name="lblUpdateNoEncrypt" VerticalAlignment="Center" />
                                <TextBlock Grid.Column="2" Text="Not Encrypted" x:Name="keyUpdateNoEncrypt" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                <TextBox Grid.Column="4" x:Name="fileUpdateNoEncrypt" IsReadOnly="True" />
                            </Grid>
                            <Grid Grid.Row="7" x:Name="grdAppleLogo" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="6" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <TextBlock Text="AppleLogo (IV)" x:Name="lblAppleLogoIV" VerticalAlignment="Center" />
                                <TextBlock Text="AppleLogo (Key)" x:Name="lblAppleLogoKey" VerticalAlignment="Center" Grid.Row="2" />
                                <TextBox Grid.Column="2" x:Name="keyAppleLogoIV" IsReadOnly="True" />
                                <TextBox Grid.Column="2" x:Name="keyAppleLogoKey" IsReadOnly="True" Grid.Row="2" />
                                <TextBox Grid.Column="4" x:Name="fileAppleLogo" IsReadOnly="True" Grid.RowSpan="3" VerticalAlignment="Center" />
                            </Grid>
                            <Grid Grid.Row="8" x:Name="grdAppleLogoNoEncrypt" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="AppleLogo" x:Name="lblAppleLogoNoEncrypt" VerticalAlignment="Center" />
                                <TextBlock Grid.Column="2" Text="Not Encrypted" x:Name="keyAppleLogoNoEncrypt" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                <TextBox Grid.Column="4" x:Name="fileAppleLogoNoEncrypt" IsReadOnly="True" />
                            </Grid>
                            <Grid Grid.Row="9" x:Name="grdBatteryCharging" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="6" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <TextBlock Text="BatteryCharging (IV)" x:Name="lblBatteryChargingIV" VerticalAlignment="Center" />
                                <TextBlock Text="BatteryCharging (Key)" x:Name="lblBatteryChargingKey" VerticalAlignment="Center" Grid.Row="2" />
                                <TextBox Grid.Column="2" x:Name="keyBatteryChargingIV" IsReadOnly="True" />
                                <TextBox Grid.Column="2" x:Name="keyBatteryChargingKey" IsReadOnly="True" Grid.Row="2" />
                                <TextBox Grid.Column="4" x:Name="fileBatteryCharging" IsReadOnly="True" Grid.RowSpan="3" VerticalAlignment="Center" />
                            </Grid>
                            <Grid Grid.Row="10" x:Name="grdBatteryChargingNoEncrypt" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="BatteryCharging0" x:Name="lblBatteryChargingNoEncrypt" VerticalAlignment="Center" />
                                <TextBlock Grid.Column="2" Text="Not Encrypted" x:Name="keyBatteryChargingNoEncrypt" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                <TextBox Grid.Column="4" x:Name="fileBatteryChargingNoEncrypt" IsReadOnly="True" />
                            </Grid>
                            <Grid Grid.Row="11" x:Name="grdBatteryCharging0" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="6" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <TextBlock Text="BatteryCharging0 (IV)" x:Name="lblBatteryCharging0IV" VerticalAlignment="Center" />
                                <TextBlock Text="BatteryCharging0 (Key)" x:Name="lblBatteryCharging0Key" VerticalAlignment="Center" Grid.Row="2" />
                                <TextBox Grid.Column="2" x:Name="keyBatteryCharging0IV" IsReadOnly="True" />
                                <TextBox Grid.Column="2" x:Name="keyBatteryCharging0Key" IsReadOnly="True" Grid.Row="2" />
                                <TextBox Grid.Column="4" x:Name="fileBatteryCharging0" IsReadOnly="True" Grid.RowSpan="3" VerticalAlignment="Center" />
                            </Grid>
                            <Grid Grid.Row="12" x:Name="grdBatteryCharging0NoEncrypt" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="BatteryCharging0" x:Name="lblBatteryCharging0NoEncrypt" VerticalAlignment="Center" />
                                <TextBlock Grid.Column="2" Text="Not Encrypted" x:Name="keyBatteryCharging0NoEncrypt" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                <TextBox Grid.Column="4" x:Name="fileBatteryCharging0NoEncrypt" IsReadOnly="True" />
                            </Grid>
                            <Grid Grid.Row="13" x:Name="grdBatteryCharging1" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="6" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <TextBlock Text="BatteryCharging1 (IV)" x:Name="lblBatteryCharging1IV" VerticalAlignment="Center" />
                                <TextBlock Text="BatteryCharging1 (Key)" x:Name="lblBatteryCharging1Key" VerticalAlignment="Center" Grid.Row="2" />
                                <TextBox Grid.Column="2" x:Name="keyBatteryCharging1IV" IsReadOnly="True" />
                                <TextBox Grid.Column="2" x:Name="keyBatteryCharging1Key" IsReadOnly="True" Grid.Row="2" />
                                <TextBox Grid.Column="4" x:Name="fileBatteryCharging1" IsReadOnly="True" Grid.RowSpan="3" VerticalAlignment="Center" />
                            </Grid>
                            <Grid Grid.Row="14" x:Name="grdBatteryCharging1NoEncrypt" Margin="0,3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{DynamicResource keyTitleWidth}" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="6" />
                                    <ColumnDefinition Width="{DynamicResource keyFilenameWidth}" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="BatteryCharging1" x:Name="lblBatteryCharging1NoEncrypt" VerticalAlignment="Center" />
